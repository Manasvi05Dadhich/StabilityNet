{
  "name": "stabilitynet-detection",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "test": "npm run build && node --test test/*.test.mjs",
    "analyze": "npm run build && node scripts/analyze.mjs",
    "analyze:interactive": "npm run build && node scripts/analyze.mjs --interactive",
    "authorize": "npm run build && node scripts/authorize.mjs",
    "publish": "npm run build && node scripts/publish.mjs",
    "watch": "npm run build && node scripts/watch.mjs",
    "demo:deploy": "node scripts/demoDeploySignals.mjs",
    "demo:emit": "node scripts/demoEmitSignals.mjs"
  },
  "dependencies": {
    "ethers": "^6.14.0",
    "viem": "^2.41.2"
  },
  "devDependencies": {
    "solc": "^0.8.31",
    "typescript": "^5.6.3"
  }
}
